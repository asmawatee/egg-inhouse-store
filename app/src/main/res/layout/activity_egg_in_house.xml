<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9B92F"
    tools:context="com.example.user.appstore.activity.EggInHouseActivity">



   <ImageView
    android:id="@+id/imageappicon"
    android:layout_width="150dp"
    android:layout_height="150dp"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="160dp"
    android:background="@drawable/eggicon" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageappicon"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="22dp"
        android:text="EGG STORE"
        android:textSize="20dp"
        android:textStyle="bold"
        android:textColor="#111111"
        android:id="@+id/textView2" />


</RelativeLayout>
